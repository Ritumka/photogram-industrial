<%= render partial: "users/links", locals: { user: @user } %>

hi<div class="row">
  <div class="col-md-6 offset-3">
    <div class="list-group">
      <% @followers.each do |follower| %>
        <%= link_to "#{follower.username}", user_path(follower.username), class: "list-group-item list-group-item-action" %>
      <% end %>
    </div>
  </div>
</div>
